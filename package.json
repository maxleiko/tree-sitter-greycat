{
  "name": "tree-sitter-greycat",
  "version": "0.1.0",
  "description": "GreyCat Language Parser",
  "repository": "https://greycat.io/",
  "license": "MIT",
  "author": {
    "name": "Maxime Tricoire",
    "email": "maxime.tricoire@datathings.com"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "greycat"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "8.3.0",
    "node-gyp-build": "4.8.4"
  },
  "devDependencies": {
    "prebuildify": "6.0.1",
    "tree-sitter-cli": "0.25.10"
  },
  "peerDependencies": {
    "tree-sitter": "0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "tree-sitter",
      "tree-sitter-cli"
    ]
  },
  "tree-sitter": [
    {
      "scope": "source.greycat",
      "injection-regex": "^greycat$"
    }
  ]
}
